.feet-img-rss
	list-style-type none
	padding 10px 15px 10px 0
	margin-bottom 0
	display flex
	align-items center
	.item
		.icon-container
			&:hover, &:active, &:focus 
				color: white
		padding-right 5px
		.fb
			background #3b5998
		.tw
			background #00acee
		.wp
			background #25D366
		.share
			text-transform uppercase
			letter-spacing -1px
			padding-right 15px
			color var(--color2-x1)
			font-family 'CeraProBold'
	
